{
  "name": "node-flow-starter",
  "version": "1.0.0",
  "description": "A simple FlowType starter kit for building node applications",
  "engines": {
    "node": ">=6"
  },
  "files": ["dist"],
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "babel src -d dist",
    "build:watch": "npm run build -- --watch",
    "format": "prettier --trailing-comma es5 --single-quote --write 'src/*/*.js'",
    "security:check": "nsp check",
    "type:check": "flow check",
    "lint": "esw 'src/*/*.js'",
    "lint:watch": "npm run lint:watch",
    "start": "run-p build:watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/admmasters/node-flow-starter.git"
  },
  "keywords": ["node", "starter-kit", "boilerplate", "flow", "flowtype"],
  "author": "Matt Revell",
  "bugs": {
    "url": "https://github.com/admmasters/node-flow-starter/issues"
  },
  "homepage": "https://github.com/admmasters/node-flow-starter#readme",
  "dependencies": {},
  "devDependencies": {
    "babel-cli": "6.26.0",
    "babel-eslint": "10.0.2",
    "babel-plugin-transform-class-properties": "6.24.1",
    "babel-preset-env": "1.7.0",
    "babel-preset-flow": "6.23.0",
    "eslint": "6.0.1",
    "eslint-config-airbnb-base": "13.2.0",
    "eslint-plugin-import": "2.18.0",
    "eslint-watch": "5.1.2",
    "npm-run-all": "4.1.5",
    "nsp": "3.2.1",
    "prettier": "1.18.2"
  }
}
